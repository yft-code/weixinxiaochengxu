/* pages/permit/permit.wxss */
.image{
  margin-top: 40rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}
.imageLarge{
  width: 100%;
  height: 100%;
}
.imgContainer{
  display: flex;
  justify-content: center;
  align-items: center;
  background-color: #F0F0F0;
  width: 340rpx;
  height: 340rpx;
  padding: 20rpx 20rpx;
  border-radius: 5rpx;
}
.message{
  margin-top: 74rpx;
}
.messageContainer{
  padding:20rpx 92rpx;
  display: flex;
  justify-content: space-around;
}
.massageDetail{
  color: #333333;
  font-weight: 600;
  font-size: 30rpx;
}
.massageTitle{
  padding-bottom: 30rpx;
  color: rgba(102,102,102,1);
  font-size: 32rpx;
}
.commonLeft{
  flex: 1.2;
}
.commonRight{
  flex: 1;
}